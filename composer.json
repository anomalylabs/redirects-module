{
    "name": "streams/redirects",
    "type": "streams-addon",
    "description": "Manage redirects from one URL or domain to another.",
    "keywords": [
        "larvel streams",
        "redirects"
    ],
    "homepage": "https://streams.dev/addons/streams/redirects",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Laravel Streams",
            "homepage": "https://streams.dev/",
            "role": "Owner"
        },
        {
            "name": "Ryan Thompson",
            "homepage": "https://ryanthepyro.com/",
            "role": "Developer"
        }
    ],
    "support": {},
    "require": {
        "streams/core": "^2.0"
    },
    "require-dev": {
        "streams/testing": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Streams\\Redirects\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Streams\\Redirects\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "laravel": {
            "providers": [
                "Streams\\Redirects\\RedirectsServiceProvider"
            ]
        }
    }
}
